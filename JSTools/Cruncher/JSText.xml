<?xml version="1.0" encoding="utf-8" ?> 
<JSCompiler>
	<language select="en-en">
		<tabNames>
			<add key="start" name="Start" />
			<add key="compile" name="Compile" />
			<add key="decompile" name="Decompile" />
			<add key="optimize" name="Optimize" />
		</tabNames>
		<start>
			<add key="start" title="Welcome to JavaScript Compiler!">Please read follow instructions before you use the compiler:</add>
			<add key="compile" title="Compile">Removes all unnecessary white spaces. After compiling, the source code will be on one line.</add>
			<add key="decompile" title="Decompile">Decompiles a JavaScript source code.</add>
			<add key="optimize" title="Optimize">Replaces all variable and method names with a random generated name.</add>
		</start>
		<compile>
			<add key="filesToCompile" value="Please select the files you d'like to compile:" />
			<add key="outputFile" value="Please choose the output destination:" />
			<add key="compileOutput" value="Compile output:" />
			<add key="choose" value="Choose" />
			<add key="add" value="Add" />
			<add key="remove" value="Remove" />
			<add key="compile" value="Compile" />
			<add key="askCompile" value="Would you like to start the compile process?" />
			<add key="compileInfo" value="Compile output:" />
		</compile>
		<ioMessages>
			<add key="fail" value="failed" />
			<add key="success" value="done" />
			<add key="open" value="Opening source files" />
			<add key="compileLine" value="Compiling line" />
			<add key="createFile" value="Creating new file" />
			<add key="compileCompleted" value="Compile process completed, total removed:" />
		</ioMessages>
	</language>
</JSCompiler>